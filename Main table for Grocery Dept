SELECT F01 AS UPC, F17 AS CATEGORY, F29 AS DESCRIPTION, F22 AS SIZE, F16 AS FAMILY
/* Select UPC, Category, Description, Size, and Family Code information.
FROM OBJ_TAB
/* Main table from SMS.
WHERE F17 BETWEEN 601001 AND 2204999
/* Set the category range for the grocery department .
ORDER BY CATEGORY;
/* Order the output based on category.
